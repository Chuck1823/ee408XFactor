<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#097601"
    tools:context="edu.clarkson.chaz.xfactor.BasicsOfMultiplication$PlaceholderFragment">

    <TextView
        android:id="@+id/section_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/explanation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="10dp"
        android:background="#097601"
        android:textColor="#FFFFFFFF"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/carryOnes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#097601"
        android:textColor="#000000"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="1dp"
        android:layout_below="@+id/explanation"
        android:layout_toEndOf="@+id/topTens"
        android:textSize="14dp"
        android:text=""/>

    <TextView
        android:id="@+id/carryTens"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#097601"
        android:textColor="#FF000000"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="1dp"
        android:layout_below="@+id/explanation"
        android:layout_toEndOf="@+id/topHundreds"
        android:textSize="14dp"
        android:text=""/>

    <TextView
        android:id="@+id/topOnes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/carryOnes"
        android:layout_centerHorizontal="true"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text="3"/>

    <TextView
        android:id="@+id/topTens"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/carryOnes"
        android:layout_toLeftOf="@id/topOnes"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text="2"/>

    <TextView
        android:id="@+id/topHundreds"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/carryOnes"
        android:layout_toLeftOf="@id/topTens"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text="1"/>

    <TextView
        android:id="@+id/bottomOnes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/topOnes"
        android:layout_centerHorizontal="true"
        android:layout_marginHorizontal="5dp"
        android:layout_marginTop="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text="5"/>

    <TextView
        android:id="@+id/bottomTens"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/topTens"
        android:layout_toLeftOf="@id/bottomOnes"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text="4"/>

    <TextView
        android:id="@+id/multiplicationSign"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@id/bottomTens"
        android:layout_below="@id/topHundreds"
        android:layout_marginRight="50dp"
        android:layout_marginTop="10dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text="x"/>

    <TextView
        android:id="@+id/multiplicationBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bottomOnes"
        android:layout_alignStart="@id/multiplicationSign"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text="_____________"/>

    <TextView
        android:id="@+id/answerTopOnes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/multiplicationBar"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text="5"/>

    <TextView
        android:id="@+id/answerTopTens"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/multiplicationBar"
        android:layout_toLeftOf="@id/answerTopOnes"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/answerTopHundreds"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/multiplicationBar"
        android:layout_toLeftOf="@id/answerTopTens"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/answerBottomOnes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/answerTopOnes"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/answerBottomTens"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/answerTopTens"
        android:layout_toLeftOf="@id/answerBottomOnes"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/answerBottomHundreds"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/answerTopHundreds"
        android:layout_toLeftOf="@id/answerBottomTens"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/answerBottomThousands"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/answerTopHundreds"
        android:layout_toLeftOf="@id/answerBottomHundreds"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/additionSign"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@id/answerBottomThousands"
        android:layout_below="@id/answerTopHundreds"
        android:layout_marginRight="50dp"
        android:layout_marginTop="10dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/additionBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/answerBottomThousands"
        android:layout_alignStart="@id/additionSign"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/finalAnswerOnes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/additionBar"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/finalAnswerTens"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/additionBar"
        android:layout_toLeftOf="@id/finalAnswerOnes"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/finalAnswerHundreds"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/additionBar"
        android:layout_toLeftOf="@id/finalAnswerTens"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/finalAnswerThousands"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/additionBar"
        android:layout_toLeftOf="@id/finalAnswerHundreds"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:background="#097601"
        android:textColor="#FF000000"
        android:textSize="25dp"
        android:text=""/>

    <TextView
        android:id="@+id/carryAdd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#097601"
        android:textColor="#000000"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="1dp"
        android:layout_below="@+id/multiplicationBar"
        android:layout_toStartOf="@id/finalAnswerThousands"
        android:textSize="14dp"
        android:text=""/>

    <TextView
        android:id="@+id/topNumLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/carryOnes"
        android:layout_toRightOf="@id/topOnes"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="15dp"
        android:background="#097601"
        android:textColor="#FFFFFFFF"
        android:textSize="18dp"
        android:text="--> Top Number"/>

    <TextView
        android:id="@+id/bottomNumLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/topOnes"
        android:layout_toRightOf="@id/bottomOnes"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:background="#097601"
        android:textColor="#ffffff"
        android:textSize="18dp"
        android:text="--> Bottom Number"/>

</RelativeLayout>
